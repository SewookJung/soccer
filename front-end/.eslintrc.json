{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-unused-vars": ["warn", { "args": "all" }]
    },
    "env": {
        "browser": true,
        "node": true
    }
}